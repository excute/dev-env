{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "shell_integration": true,
  "patch_pwsh_bleed": true,
  "version": 3,
  "transient_prompt": {
    "foreground": "darkGray",
    "template": "\n {{ .Segments.Status.String }}  {{ .Segments.ExecutionTime.FormattedMs }}\n\n<black,lightYellow>  {{ now | date \"15:04:05\" }} </><lightYellow> ",
    "filler": "",
    "newline": false
  },
  "blocks": [
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "alias": "Status",
          "style": "plain",
          "foreground": "darkGray",
          "template": " {{ .String }}",
          "properties": {
            "always_enabled": true,
            "status_template": "{{ if eq .Code 0 }}<green> {{ else }}<red> {{ .Code | hresult }}{{ end }}</>",
            "status_separator": " | "
          }
        },
        {
          "type": "executiontime",
          "alias": "ExecutionTime",
          "style": "plain",
          "foreground": "darkGray",
          "template": "  {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true,
            "threshold": 500,
            "style": "austin"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "template": "\n",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "git",
          "alias": "Git",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "black",
          "background": "green",
          "template": " {{ .UpstreamIcon }}{{ url (base .UpstreamURL) .UpstreamURL }} {{ .HEAD }} ",
          "properties": {
            "fetch_status": false,
            "fetch_stash_count": false,
            "fetch_worktree_count": false,
            "fetch_upstream_icon": true,
            "fetch_bare_info": false,
            "fetch_user": false,
            "branch_max_length": 0,
            "branch_icon": " ",
            "branch_identical_icon": "≡ ",
            "branch_ahead_icon": "↑ ",
            "branch_behind_icon": "↓ ",
            "branch_gone_icon": "≢ ",
            "commit_icon": " ",
            "tag_icon": " ",
            "rebase_icon": " ",
            "cherry_pick_icon": " ",
            "revert_icon": " ",
            "merge_icon": " ",
            "no_commits_icon": " ",
            "github_icon": " ",
            "gitlab_icon": " ",
            "bitbucket_icon": " ",
            "azure_devops_icon": " ",
            "codecommit_icon": " ",
            "git_icon": " "
          }
        },
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "black",
          "background": "lightYellow",
          "template": "{{ if .Segments.Contains \"Git\" }}{{ else }}  {{ end }}"
        },
        {
          "type": "path",
          "alias": "Path",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "white",
          "background": "darkGray",
          "template": " {{ path .Path .Location }} ",
          "properties": {
            "folder_separator_icon": "/",
            "folder_separator_template": "",
            "home_icon": "~",
            "folder_icon": " ",
            "windows_registry_icon": " ",
            "style": "powerlevel",
            "mixed_threshold": 4,
            "max_depth": 1,
            "max_width": 80,
            "hide_root_location": false,
            "cycle": [],
            "cycle_folder_separator": false,
            "folder_format": "%s",
            "edge_format": "%s",
            "left_format": "%s",
            "right_format": "%s",
            "gitdir_format": "<red>%s</>",
            "display_cygpath": false,
            "display_root": false,
            "mapped_locations_enabled": true,
            "mapped_locations": {
              "{{ .Segments.Git.Dir }}": ""
            }
          }
        }
      ]
    },
    {
      "type": "prompt",
      "newline": false,
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "black",
          "background": "lightYellow",
          "template": "  {{ .UserName }}{{ if .Root }}  {{ end }}{{ if .SSHSession }} 󰢩 {{ end }}  {{ .HostName }} "
        },
        {
          "type": "sysinfo",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "white",
          "background": "darkGray",
          "template": "   {{ round .PhysicalPercentUsed .Precision }}% ",
          "properties": {
            "precision": 2
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "black",
          "background": "lightRed"
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "black",
          "background": "lightYellow",
          "template": " {{ .Icon }}",
          "properties": {
            "display_distro_name": false,
            "macos": " ",
            "linux": " ",
            "windows": " ",
            "alpine": " ",
            "aosc": " ",
            "arch": " ",
            "centos": " ",
            "coreos": " ",
            "debian": " ",
            "devuan": " ",
            "raspbian": " ",
            "elementary": " ",
            "fedora": " ",
            "gentoo": " ",
            "mageia": " ",
            "manjaro": " ",
            "mint": " ",
            "nixos": " ",
            "opensuse": " ",
            "redhat": " ",
            "sabayon": " ",
            "slackware": " ",
            "ubuntu": " ",
            "rocky": " ",
            "alma": " ",
            "android": " "
          }
        },
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "black",
          "background": "lightYellow",
          "template": "{{ .Name }}",
          "properties": {
            "mapped_shell_names": {
              "pwsh": " ",
              "bash": " ",
              "zsh": "󰬡 ",
              "fish": " "
            }
          }
        },
        {
          "type": "upgrade",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "black",
          "background": "blue",
          "template": "  ",
          "properties": {
            "cache_duration": "48h"
          }
        }
      ]
    }
  ],
  "tooltips": [
    {
      "type": "go",
      "style": "plain",
      "foreground": "darkGray",
      "template": " {{ if .Error }} {{ .Error }}{{ else }}{{ url .Full .URL }}{{ end }} ",
      "tips": [
        "go"
      ],
      "properties": {
        "home_enabled": false,
        "fetch_version": true,
        "display_mode": "context",
        "cache_version": false,
        "parse_mod_file": false
      }
    },
    {
      "type": "node",
      "style": "plain",
      "foreground": "darkGray",
      "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ url .Full .URL }}{{ end }} ",
      "tips": [
        "node",
        "nvm"
      ],
      "properties": {
        "display_mode": "context",
        "fetch_version": true,
        "home_enabled": false,
        "cache_version": false,
        "fetch_package_manager": false,
        "yarn_icon": "B",
        "npm_icon": " "
      }
    },
    {
      "type": "npm",
      "style": "plain",
      "foreground": "darkGray",
      "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ url .Full .URL }}{{ end }} ",
      "tips": [
        "npm"
      ],
      "properties": {
        "home_enabled": false,
        "fetch_version": true,
        "display_mode": "context",
        "cache_version": false
      }
    },
    {
      "type": "kubectl",
      "style": "plain",
      "foreground": "darkGray",
      "template": "󱃾 {{ .Context }}{{ if .Namespace }} :: {{ .Namespace }}{{ end }} ",
      "tips": [
        "kubectl"
      ]
    },
    {
      "type": "git",
      "style": "plain",
      "tips": [
        "git"
      ],
      "foreground": "darkGray",
      "template": "≡ ",
      "templates_logic": "join",
      "templates": [
        "{{ if .Working.Changed }} {{ .Working.String }}{{ end }}",
        "{{ if gt .StashCount 0 }} {{ .StashCount }}{{ end }}",
        "{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}",
        "{{ if gt .Behind 0 }} {{ .Behind }}{{ end }}",
        "{{ if gt .Ahead 0 }} {{ .Ahead }}{{ end }}"
      ],
      "properties": {
        "fetch_status": true,
        "fetch_stash_count": true,
        "fetch_worktree_count": false,
        "fetch_upstream_icon": false,
        "fetch_bare_info": false,
        "fetch_user": false,
        "branch_max_length": 0,
        "branch_icon": " ",
        "branch_identical_icon": "≡ ",
        "branch_ahead_icon": "↑ ",
        "branch_behind_icon": "↓ ",
        "branch_gone_icon": "≢ ",
        "commit_icon": " ",
        "tag_icon": " ",
        "rebase_icon": " ",
        "cherry_pick_icon": " ",
        "revert_icon": " ",
        "merge_icon": " ",
        "no_commits_icon": " ",
        "github_icon": " ",
        "gitlab_icon": " ",
        "bitbucket_icon": " ",
        "azure_devops_icon": " ",
        "codecommit_icon": " ",
        "git_icon": " "
      }
    }
  ],
  "secondary_prompt": {
    "foreground": "blue",
    "template": "  "
  }
}